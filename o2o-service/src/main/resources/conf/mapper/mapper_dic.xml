<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ihomefnt.o2o.intf.dao.dic.DictionaryDao">


    <select id="getValueByKey" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT dic_value as value FROM t_dictionary WHERE dic_key=#{key}
    </select>

</mapper>